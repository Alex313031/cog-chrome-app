{
  "manifest_version": 3,
  "key": "",
  "name": "COG - System Info Viewer",
  "short_name": "COG",
  "description": "__MSG_appDescription__",
  "version": "0.15.1",
  "offline_enabled": true,
  "minimum_chrome_version": "88",
  "version_name": "0.15.1",
  "default_locale": "en",
  "icons": {
    "16": "assets/icon_16.png",
    "24": "assets/icon_24.png",
    "32": "assets/icon_32.png",
    "36": "assets/icon_36.png",
    "48": "assets/icon_48.png",
    "64": "assets/icon_64.png",
    "78": "assets/icon_78.png",
    "96": "assets/icon_96.png",
    "128": "assets/icon_128.png",
    "144": "assets/icon_144.png",
    "256": "assets/icon_256.png",
    "512": "assets/icon_512.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage"
  ]
}
